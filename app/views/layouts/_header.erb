<div style="background-color: aquamarine">
  <% if logged_in? %>
    <%= link_to current_user.name, user_path(id: current_user) %>
    <%= link_to 'コード提出', new_code_path %>
    <%= link_to 'Logout', logout_path, method: :delete %>
  <% else %>
    <%= link_to 'Login', login_path %>
  <% end %>
  <%= link_to 'コード一覧', codes_path %>
</div>
